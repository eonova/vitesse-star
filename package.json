{
  "name": "vitesse-star",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:fix": "eslint src/**/*.*  --fix",
    "check:knip": "knip",
    "check:spelling": "cspell -c .cspell.json --no-progress --no-summary --no-must-find-files --unique",
    "check:style": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest --mode development",
    "prepare": "simple-git-hooks",
    "deps:up": "taze major -I"
  },
  "dependencies": {
    "@ameinhardt/unocss-preset-daisy": "catalog:frontend",
    "@vueuse/core": "catalog:frontend",
    "alova": "catalog:frontend",
    "daisyui": "catalog:frontend",
    "vue": "catalog:frontend",
    "vue-router": "catalog:frontend"
  },
  "devDependencies": {
    "@alova/adapter-axios": "catalog:dev",
    "@eonova/eslint-config": "catalog:dev",
    "@iconify-json/carbon": "catalog:build",
    "@iconify/tailwind4": "catalog:build",
    "@types/node": "catalog:dev",
    "@typescript-eslint/eslint-plugin": "catalog:dev",
    "@typescript-eslint/parser": "catalog:dev",
    "@unocss/eslint-plugin": "catalog:dev",
    "@vitejs/plugin-vue": "catalog:build",
    "@vue-macros/volar": "catalog:dev",
    "@vue/test-utils": "catalog:dev",
    "cspell": "catalog:dev",
    "dotenv": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-define-config": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "jsdom": "catalog:build",
    "knip": "catalog:dev",
    "lint-staged": "catalog:dev",
    "pinia": "catalog:dev",
    "pinia-auto-refs": "catalog:dev",
    "pinia-plugin-persistedstate": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:build",
    "unplugin-auto-import": "catalog:build",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "catalog:build",
    "unplugin-vue-macros": "catalog:build",
    "unplugin-vue-router": "catalog:build",
    "vite": "catalog:build",
    "vite-plugin-vue-devtools": "catalog:build",
    "vitest": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm tsx ./scripts/verify-commit.ts"
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": "eslint --fix"
  }
}
