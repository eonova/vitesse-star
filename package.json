{
  "name": "vitesse-star",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:fix": "eslint src/**/*.*  --fix",
    "check:knip": "knip",
    "check:spelling": "cspell -c .cspell.json --no-progress --no-summary --no-must-find-files --unique",
    "check:style": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest --mode development",
    "prepare": "simple-git-hooks",
    "deps:up": "taze major -I"
  },
  "dependencies": {
    "@ameinhardt/unocss-preset-daisy": "^1.1.7",
    "@vueuse/core": "catalog:frontend",
    "alova": "^3.2.11",
    "daisyui": "^5.0.37",
    "vue": "catalog:frontend",
    "vue-router": "catalog:frontend"
  },
  "devDependencies": {
    "@alova/adapter-axios": "^2.0.13",
    "@eonova/eslint-config": "^0.3.1",
    "@iconify-json/carbon": "catalog:build",
    "@iconify/tailwind4": "^1.0.6",
    "@types/node": "catalog:dev",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@unocss/eslint-plugin": "catalog:dev",
    "@vitejs/plugin-vue": "catalog:build",
    "@vue-macros/volar": "catalog:dev",
    "@vue/test-utils": "catalog:dev",
    "cspell": "^9.0.2",
    "dotenv": "^16.5.0",
    "eslint": "catalog:dev",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-format": "catalog:dev",
    "jsdom": "catalog:build",
    "knip": "^5.58.0",
    "lint-staged": "catalog:dev",
    "pinia": "^3.0.2",
    "pinia-auto-refs": "^0.0.12",
    "pinia-plugin-persistedstate": "^4.3.0",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:build",
    "unplugin-auto-import": "catalog:build",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "catalog:build",
    "unplugin-vue-macros": "catalog:build",
    "unplugin-vue-router": "catalog:build",
    "vite": "catalog:build",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vitest": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm tsx ./scripts/verify-commit.ts"
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": "eslint --fix"
  }
}
